{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/server.ts",
    "build": "rimraf dist && swc src -d dist --source-maps --copy-files && npm run build:tsc",
    "build:tsc": "tsc && tsc-alias",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:revert": "npx sequelize-cli db:migrate:undo",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo:all",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npm run db:migrate && npm run seed:all",
    "start": "node dist/index.js",
    "seed:all": "npm run seed:role && npm run seed:feature && npm run seed:permission && npm run seed:rolePermission && npm run seed:users",
    "seed:role": "ts-node ./src/sequelizeDir/seeders/role.seeder.ts",
    "seed:feature": "ts-node ./src/sequelizeDir/seeders/feature.seeder.ts",
    "seed:permission": "ts-node ./src/sequelizeDir/seeders/permission.seeder.ts",
    "seed:rolePermission": "ts-node ./src/sequelizeDir/seeders/rolePermission.seeder.ts",
    "seed:users": "ts-node ./src/sequelizeDir/seeders/users.seeder.ts",
    "format": "npx prettier --write ./src  --config ./.prettierrc",
    "git:sync": "git pull origin develop && npm run db:migrate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/axios": "^0.9.36",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/winston": "^2.4.4",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.1",
    "express": "^5.2.1",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "magic-bytes.js": "^1.12.1",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "slugify": "^1.6.6",
    "ts-dotenv": "^0.9.1",
    "tslib": "^2.8.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/bcrypt": "^6.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/sequelize": "^4.28.20",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  }
}
